{
  "name": "@14islands/r3f-scroll-rig",
  "version": "8.9.1",
  "description": "Progressively enhance any React website with WebGL using @react-three/fiber",
  "private": false,
  "type": "module",
  "source": "src/index.ts",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "sideEffects": false,
  "exports": {
    "./css": {
      "default": "./dist/style.css"
    },
    "./scrollbar": {
      "types": "./dist/scrollbar/index.d.ts",
      "require": "./dist/scrollbar/index.cjs",
      "import": "./dist/scrollbar/index.js"
    },
    "./powerups": {
      "types": "./dist/powerups/index.d.ts",
      "require": "./dist/powerups/index.cjs",
      "import": "./dist/powerups/index.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc &&",
    "dev": "microbundle watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/14islands/r3f-scroll-rig.git"
  },
  "keywords": [
    "@react-three/fiber",
    "webgl",
    "react",
    "three"
  ],
  "author": "@14islands",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/14islands/r3f-scroll-rig/issues"
  },
  "homepage": "https://github.com/14islands/r3f-scroll-rig#readme",
  "dependencies": {
    "@14islands/lerp": "^1.0.3",
    "@juggle/resize-observer": "^3.4.0",
    "@studio-freight/lenis": "^0.2.28",
    "@types/query-string": "^6.3.0",
    "debounce": "^1.2.1",
    "fast-deep-equal": "^3.1.3",
    "microbundle": "^0.15.1",
    "query-string": "^6.14.1",
    "react-intersection-observer": "^9.4.0",
    "react-merge-refs": "^1.1.0",
    "supports-webp": "^3.0.0",
    "suspend-react": "^0.0.8",
    "vecn": "^1.3.1",
    "zustand": "^3.4.2"
  },
  "devDependencies": {
    "@react-three/drei": "^9.40.0",
    "@react-three/fiber": "^8.9.2",
    "@types/debounce": "^1.2.1",
    "@types/react": "^18.0.25",
    "@types/three": "^0.149.0",
    "microbundle": "^0.15.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "three": "^0.144.0",
    "vite": "^4.1.4"
  },
  "peerDependencies": {
    "@react-three/drei": ">=9.0.0",
    "@react-three/fiber": ">=8.0.0",
    "react": ">=18.0",
    "react-dom": ">=18.0",
    "three": ">=0.139.0"
  }
}
