{
  "name": "@14islands/r3f-scroll-rig",
  "version": "8.5.9",
  "description": "Progressively enhance any React website with WebGL using @react-three/fiber",
  "private": false,
  "type": "module",
  "source": "src/index.ts",
  "main": "./dist/scrollrig.cjs",
  "module": "./dist/scrollrig.module.js",
  "unpkg": "./dist/scrollrig.umd.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/scrollrig.js",
    "default": "./dist/scrollrig.modern.mjs"
  },
  "scripts": {
    "build": "microbundle --globals @react-three/fiber=@react-three/fiber,@juggle/resize-observer=@juggle/resize-observer,@studio-freight/lenis=@studio-freight/lenis,react/jsx-runtime=jsx",
    "buildOLD": "microbundle --globals @react-three/fiber=@react-three/fiber,@juggle/resize-observer=@juggle/resize-observer,@studio-freight/lenis=@studio-freight/lenis,react/jsx-runtime=jsx --jsx React.createElement --jsxFragment React.Fragment --jsxImportSource react",
    "dev": "microbundle watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/14islands/r3f-scroll-rig.git"
  },
  "keywords": [
    "@react-three/fiber",
    "webgl",
    "react",
    "three"
  ],
  "author": "@14islands",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/14islands/r3f-scroll-rig/issues"
  },
  "homepage": "https://github.com/14islands/r3f-scroll-rig#readme",
  "dependencies": {
    "@14islands/lerp": "^1.0.3",
    "@juggle/resize-observer": "^3.4.0",
    "@studio-freight/lenis": "^0.2.11",
    "@types/query-string": "^6.3.0",
    "debounce": "^1.2.1",
    "fast-deep-equal": "^3.1.3",
    "query-string": "^6.14.1",
    "react-intersection-observer": "^9.4.0",
    "react-merge-refs": "^1.1.0",
    "react-use": "^17.4.0",
    "supports-webp": "^3.0.0",
    "suspend-react": "^0.0.8",
    "vecn": "^1.3.1",
    "zustand": "^3.4.2"
  },
  "devDependencies": {
    "@types/debounce": "^1.2.1",
    "microbundle": "^0.15.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "three": "^0.144.0"
  },
  "peerDependencies": {
    "@react-three/drei": ">=9.0.0",
    "@react-three/fiber": ">=8.0.0",
    "react": ">=18.0",
    "react-dom": ">=18.0",
    "three": ">=0.139.0"
  }
}
